{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "filters",
      "ingredients",
      "_id",
      "name"
    ],
    "properties": {
      "filters": {
        "$id": "#/properties/filters",
        "type": "array",
        "title": "The Filters Schema"
      },
      "ingredients": {
        "$id": "#/properties/ingredients",
        "type": "array",
        "title": "The Ingredients Schema"
      },
      "_id": {
        "$id": "#/properties/_id",
        "type": "string",
        "title": "The _id Schema",
        "default": "",
        "examples": [
          "5bf96f2009fc72171c6dac24"
        ],
        "pattern": "^(.*)$"
      },
      "name": {
        "$id": "#/properties/name",
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "Test List"
        ],
        "pattern": "^(.*)$"
      }
    }
  }